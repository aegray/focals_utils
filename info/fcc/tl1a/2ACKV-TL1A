<script type="afb9c43a4a468dbd0125010b-text/javascript">
function subscribe(){
$("#ajaxform").submit(function(e)
{
    var postData = $(this).serializeArray();
    var formURL = $(this).attr("action");
    $.ajax(
    {
        url : formURL,
        type: "POST",
        data : postData,
        success:function(data, textStatus, jqXHR)
        {
                document.getElementById("subform").innerHTML = data;
        },
        error: function(jqXHR)
        {
                document.getElementById("subform").innerHTML = jqXHR.responseText;
        }
    });
    e.preventDefault();
    e.unbind();
});

$("#ajaxform").submit();
}
$(document).on("keypress", "form", function(event) {
    if (event.keyCode == 13){
     subscribe();
    }
       return event.keyCode != 13;
});
</script>
